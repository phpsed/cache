{
    "name": "phpsed/cache",
    "description": "Symfony response cache",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": [
        "phpsed",
        "cache",
        "symfony",
        "redis",
        "doctrine"
    ],
    "require": {
        "php": "^7.1.3",
        "symfony/cache": "*",
        "symfony/config": "*",
        "symfony/dependency-injection": "*",
        "symfony/event-dispatcher": "*",
        "symfony/http-foundation": "*",
        "symfony/http-kernel": "*",
        "doctrine/annotations": "^1.6",
        "doctrine/orm": "^2.6",
        "predis/predis": "^1"
    },
    "suggest": {
        "snc/redis-bundle": "For easier Predis client configuration"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Phpsed\\Cache\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "symfony": {
            "require": "4.1.*"
        }
    }
}
